{
  "name": "cb/test",
  "description": "A test project.",
  "type": "project",
  "require": {
    "php": ">=7.2.0",
    "symfony/validator": "^4.2",
    "symfony/http-foundation": "^4.1",
    "doctrine/dbal": "^2.6",
    "psr/container": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "System\\": "system/",
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {

  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
